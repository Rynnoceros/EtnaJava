<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductsServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.ProductsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WhoAreWeServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.WhoAreWeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContactServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.ContactServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BasketServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.BasketServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PersonnalInformationServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.PersonnalInformationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailProductServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.DetailProductServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientAdresseServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.ClientAdresseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/IndexServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductsServlet</servlet-name>
        <url-pattern>/ProductsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WhoAreWeServlet</servlet-name>
        <url-pattern>/WhoAreWeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContactServlet</servlet-name>
        <url-pattern>/ContactServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BasketServlet</servlet-name>
        <url-pattern>/BasketServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PersonnalInformationServlet</servlet-name>
        <url-pattern>/PersonnalInformationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailProductServlet</servlet-name>
        <url-pattern>/DetailProductServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientAdresseServlet</servlet-name>
        <url-pattern>/ClientAdresseServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>LoginServlet</welcome-file>
    </welcome-file-list>
</web-app>
