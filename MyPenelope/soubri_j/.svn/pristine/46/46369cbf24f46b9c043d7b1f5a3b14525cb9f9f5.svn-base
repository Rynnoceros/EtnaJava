package com.etna.mypenelope.core.entities;

/**
 * Message destinataire class.
 * @author soubri_j/martin_m
 */
public class MessageDestinataire {
    protected Integer idMessageDestinataire;
    protected Integer idMessage;
    protected Integer idUtilisateur;

    public void setIdMessageDestinataire(Integer idMessageDestinataire) {
        this.idMessageDestinataire = idMessageDestinataire;
    }

    public void setIdMessage(Integer idMessage) {
        this.idMessage = idMessage;
    }

    public void setIdUtilisateur(Integer idUtilisateur) {
        this.idUtilisateur = idUtilisateur;
    }

    public Integer getIdMessageDestinataire() {
        return idMessageDestinataire;
    }

    public Integer getIdMessage() {
        return idMessage;
    }

    public Integer getIdUtilisateur() {
        return idUtilisateur;
    }
}
