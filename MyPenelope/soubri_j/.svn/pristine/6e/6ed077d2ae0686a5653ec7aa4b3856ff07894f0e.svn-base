<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="com.etna.mypenelope.modules.messages.FXMLMessageManagementController">
   <children>
      <Separator layoutX="160.0" orientation="VERTICAL" prefHeight="400.0" prefWidth="5.0" />
      <ListView fx:id="lvMessageType" prefHeight="400.0" prefWidth="162.0"/>
      <ListView fx:id="lvMessages" layoutX="163.0" layoutY="31.0" prefHeight="369.0" prefWidth="437.0" />
      <Button layoutX="166.0" layoutY="2.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="134.0" text="New Message" />
      <Button layoutX="306.0" layoutY="2.0" mnemonicParsing="false" 
              prefHeight="27.0" prefWidth="140.0" text="Delete Message" 
              onAction="#deleteMessage" />
   </children>
</AnchorPane>
